{
	"compilerOptions": {
		"allowJs": false,
		"composite": false,
		"declaration": false,
		"declarationMap": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": false,
		"lib": ["ES2023"],
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"noImplicitAny": true,
		"resolveJsonModule": true,
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"sourceMap": false,
		"strict": true,
    "target": "ES2022"
	}
}